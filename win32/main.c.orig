int ELFHash(char str[])                        //字符串加密
{
    int hash=0 ;
    int x=0 ;
    int i ;
    for(i=0; i<strlen(str); i++)
    {
        hash=(hash<<4)+str[i];
        if((x=(int)(hash&0xF0000000L))!=0)
        {
            hash^=(x>>24);
            hash&=~x ;
        }
    }
    return(hash&0x7FFFFFFF);
}

int Option(char rw,int nb,int n)               //设置文件
{
    void Findoutoption();
    void Division();
    int nu[3];
    FILE *fp;
    if((fp=fopen("option.ini","r"))==NULL)
    {
        fclose(fp);
        Findoutoption();
        fp=fopen("option.ini","r");
    }
    fscanf(fp,"num = %d\nsum = %d\npassword = %d",&nu[0],&nu[1],&nu[2]);
    fclose(fp);
    switch(rw)
    {
    case 'r':
        switch(n)
        {
        case 0:
            if(nu[0]==0)return 1;
            else return nu[0];
            break;
        case 1:
            return nu[1];
            break;
        case 2:
            return nu[2];
            break;
        }
        break;
    case 'w':
        if((fp=fopen("option.ini","w"))==NULL)
        {
            fclose(fp);
            MessageBox(NULL,"< 文件错误:设置文件未能创建成功 >","设置",MB_OK);
            exit(0);
        }
        switch(n)
        {
        case 0:
            if(nb==0)nu[0]=1;
            else nu[0]=nb;
            break;
        case 1:
            nu[1]=nb;
            break;
        case 2:
            nu[2]=nb;
            break;
        }
        fprintf(fp,"num = %d\nsum = %d\npassword = %d",nu[0],nu[1],nu[2]);
        fclose(fp);
        return 1;
    }
    return 0;
}

void Division()                                //初始化题库
{
    FILE *fpin,*fpout;
    char c_temp,files[]="select001.db";
    int num_titles,num_subject=0;
    if((fpin=fopen("select.db","r"))==NULL)
    {
        fclose(fpin);
        MessageBox(NULL,"< division:题库文件未找到 >","初始化题库",MB_OK);
        exit(0);
    }
    while(!feof(fpin))
    {
        num_titles=0;
        if(files[8]>'9')
        {
            files[7]++;
            files[8]='0';
        }
        if(files[7]>'9')
        {
            files[6]++;
            files[7]='0';
        }
        if(files[6]>'9')break;
        fpout=fopen(files,"w");
        while(!feof(fpin))
        {
            fputc(c_temp=fgetc(fpin),fpout);
            if(c_temp=='^')
            {
                num_titles++;
                num_subject++;
            }
            if(num_titles==500)
            {
                fputc(fgetc(fpin),fpout);
                fputc(fgetc(fpin),fpout);
                break;
            }
        }
        fclose(fpout);
        files[8]++;
    }
    fclose(fpin);
    if(fopen("option.ini","r")==NULL)
    {
        fpin=fopen("option.ini","w");
        fprintf(fpin,"num = 1\nsum = 10\npassword = 6796286");
        fclose(fpin);
    }
    Option('w',num_subject,1);
}

void Filesopen(char *file)                     //导入题库
{
    FILE *fpin,*fpout;
    char c_temp,files[]="select001.db";
    int num_titles,num_subject=0;
